<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="dialogAnchor" xmlns:fx="http://javafx.com/fxml/1"
	xmlns="http://javafx.com/javafx/2.2">
	<!-- TODO Add Nodes -->
	<children>
		<GridPane layoutY="0.0" prefHeight="405.0"
			AnchorPane.leftAnchor="20.0">
			<children>

				<Label text="Name" GridPane.columnIndex="0"
					GridPane.halignment="CENTER" GridPane.rowIndex="2" />
				<TextField fx:id="txtName" prefWidth="200.0"
					GridPane.columnIndex="1" GridPane.rowIndex="2" />
				<Label text="Street" GridPane.columnIndex="0"
					GridPane.halignment="CENTER" GridPane.rowIndex="3" />
				<TextField fx:id="txtStreet" prefWidth="200.0"
					GridPane.columnIndex="1" GridPane.rowIndex="3" />
				<Label text="City" GridPane.columnIndex="0"
					GridPane.halignment="CENTER" GridPane.rowIndex="4" />
				<TextField fx:id="txtCity" prefWidth="200.0"
					GridPane.columnIndex="1" GridPane.rowIndex="4" />
				<Label text="State" GridPane.columnIndex="0"
					GridPane.halignment="CENTER" GridPane.rowIndex="5" />
				<TextField fx:id="txtState" prefWidth="200.0"
					GridPane.columnIndex="1" GridPane.rowIndex="5" />
				<Label text="Zip" GridPane.columnIndex="0"
					GridPane.halignment="CENTER" GridPane.rowIndex="6" />
				<TextField fx:id="txtZip" prefWidth="200.0"
					GridPane.columnIndex="1" GridPane.rowIndex="6" />

				<Label text="Email" GridPane.columnIndex="0"
					GridPane.halignment="CENTER" GridPane.rowIndex="7" />
				<TextField fx:id="txtEMail" prefWidth="200.0"
					GridPane.columnIndex="1" GridPane.rowIndex="7" />

				<Label text="CC Number" GridPane.columnIndex="0"
					GridPane.halignment="CENTER" GridPane.rowIndex="8" />
				<TextField fx:id="txtCCNumber" prefWidth="200.0"
					GridPane.columnIndex="1" GridPane.rowIndex="8" />

				<Label text="Exp. Date" GridPane.columnIndex="0"
					GridPane.halignment="CENTER" GridPane.rowIndex="9" />
				<TextField fx:id="txtExpDate" prefWidth="200.0"
					GridPane.columnIndex="1" GridPane.rowIndex="9" />

				<RadioButton mnemonicParsing="false" text="Gold"
					GridPane.columnIndex="1" GridPane.rowIndex="0" fx:id="goldRB">
					<toggleGroup>
						<ToggleGroup fx:id="AccountType" />
					</toggleGroup>
				</RadioButton>
				<RadioButton mnemonicParsing="false" text="Silver"
					toggleGroup="$AccountType" GridPane.columnIndex="1"
					GridPane.rowIndex="0" fx:id="silverRB">
					<GridPane.margin>
						<Insets top="40.0" />
					</GridPane.margin>
				</RadioButton>

				<RadioButton mnemonicParsing="false" text="Bronze"
					toggleGroup="$AccountType" GridPane.columnIndex="1"
					GridPane.rowIndex="0" fx:id="bronzeRB">
					<GridPane.margin>
						<Insets top="80.0" />
					</GridPane.margin>
				</RadioButton>

				<Button fx:id="btnOk" mnemonicParsing="false" prefWidth="74.0"
					text="OK" GridPane.columnIndex="1" GridPane.rowIndex="10" onAction="#Submit" />
				<Button fx:id="btnCancel" mnemonicParsing="false" prefWidth="68.0"
					text="Cancel" GridPane.columnIndex="1" GridPane.rowIndex="10"
					onAction="#closeDialog">
					<GridPane.margin>
						<Insets left="80.0" />
					</GridPane.margin>
				</Button>
			</children>
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" maxWidth="100.0"
					minWidth="10.0" prefWidth="61.0" />
				<ColumnConstraints hgrow="SOMETIMES" maxWidth="188.0"
					minWidth="10.0" prefWidth="188.0" />
				<ColumnConstraints hgrow="SOMETIMES" maxWidth="100.0"
					minWidth="10.0" prefWidth="51.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints maxHeight="60.0" minHeight="10.0"
					prefHeight="24.0" vgrow="SOMETIMES" />
				<RowConstraints maxHeight="36.0" minHeight="0.0"
					prefHeight="36.0" vgrow="SOMETIMES" />
				<RowConstraints maxHeight="30.0" minHeight="10.0"
					prefHeight="30.0" vgrow="SOMETIMES" />
				<RowConstraints maxHeight="30.0" minHeight="10.0"
					prefHeight="30.0" vgrow="SOMETIMES" />
				<RowConstraints maxHeight="30.0" minHeight="10.0"
					prefHeight="30.0" vgrow="SOMETIMES" />
				<RowConstraints maxHeight="30.0" minHeight="10.0"
					prefHeight="30.0" vgrow="SOMETIMES" />
				<RowConstraints maxHeight="30.0" minHeight="10.0"
					prefHeight="30.0" vgrow="SOMETIMES" />
				<RowConstraints maxHeight="30.0" minHeight="10.0"
					prefHeight="30.0" vgrow="SOMETIMES" />
				<RowConstraints maxHeight="30.0" minHeight="10.0"
					prefHeight="30.0" vgrow="SOMETIMES" />
				<RowConstraints maxHeight="30.0" minHeight="10.0"
					prefHeight="30.0" vgrow="SOMETIMES" />
				<RowConstraints maxHeight="30.0" minHeight="10.0"
					prefHeight="30.0" vgrow="SOMETIMES" />
			</rowConstraints>
		</GridPane>
	</children>
</AnchorPane>
