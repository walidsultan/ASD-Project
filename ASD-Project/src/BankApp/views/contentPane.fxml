<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<fx:root type="javafx.scene.layout.VBox" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <TableView fx:id="tblAccounts" prefHeight="200.0" prefWidth="450.0">
    <columns>
      <TableColumn prefWidth="75.0" text="AccountNo" fx:id="accountNoCol" />
      <TableColumn prefWidth="75.0" text="Name" fx:id="nameCol" />
      <TableColumn prefWidth="75.0" text="City" fx:id="cityCol"/>
      <TableColumn prefWidth="75.0" text="Amount" fx:id="amountCol"/>
      <TableColumn prefWidth="75.0" text="P/C" fx:id="pcCol"/>
      <TableColumn prefWidth="75.0" text="CH/S" fx:id="chsCol" />
    </columns>
    <VBox.margin>
      <Insets top="20.0" />
    </VBox.margin>
  </TableView>
</fx:root>
